{
  "expo": {
    "runtimeVersion": "1.0.0"
  },
  "cli": {
    "version": ">= 16.15.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "GOOGLE_MAPS_API_KEY": "INSERT_KEY_HERE"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
