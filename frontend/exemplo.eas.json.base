{
  "cli": {
    "version": ">= 16.15.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "env": {
        "GOOGLE_CLIENT_ID": "INSERT_ID_HERE"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "GOOGLE_CLIENT_ID": "INSERT_ID_HERE"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "GOOGLE_CLIENT_ID": "INSERT_ID_HERE"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
